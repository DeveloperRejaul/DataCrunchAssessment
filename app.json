{
  "expo": {
    "name": "DataCrunchAssessment",
    "slug": "DataCrunchAssessment",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "datacrunchassessment",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.DataCrunchAssessment",
      "icon": {
        "dark": "./src/core/assets/images/ios-dark.png",
        "light": "./src/core/assets/images/ios-light.png",
        "tined": "./src/core/assets/images/ios-tinted.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/core/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "icon":"./src/core/assets/images/icon.png",
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.DataCrunchAssessment"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/core/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/core/assets/images/splash-icon-dark.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "image": "./src/core/assets/images/splash-icon-dark.png",
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a7539f0b-2eeb-42cf-9f8c-0f13dd150295"
      }
    }
  }
}
